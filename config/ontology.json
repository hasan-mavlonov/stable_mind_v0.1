{
  "version": "0.1",
  "scale": {
    "min": -1.0,
    "max": 1.0,
    "neutral": 0.0,
    "rule": "Only output a dimension value when explicitly supported by text. If uncertain, omit.",
    "interpretation": {
      "-1.0": "strongly low on the dimension's 'high' direction",
      "0.0": "neutral/unknown (but do not output unless explicitly stated)",
      "1.0": "strongly high on the dimension's 'high' direction"
    }
  },
  "entity_types": {
