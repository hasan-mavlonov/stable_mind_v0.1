        "quietness": {
          "description": "How quiet vs noisy the place is.",
          "polarity_high": "quiet",
          "polarity_low": "noisy",
          "examples_high": [
            "silent",
            "peaceful",
            "calm",
            "no sound"
          ],
          "examples_low": [
            "loud",
            "noisy",
            "chaotic",
            "blasting music"
          ]
        },
        "brightness": {
          "description": "How bright vs dim the place feels.",
          "polarity_high": "bright",
          "polarity_low": "dim",
          "examples_high": [
